{
  "name": "specialisering-1st",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "firebase": "6.3.5",
    "moment": "2.24.0",
    "node-sass": "4.12.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-icons": "3.7.0",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-toastify": "^5.4.1",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "@testing-library/react": "^9.1.3",
    "eslint": "^6.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --watchAll",
    "eject": "react-scripts eject"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/**/*.jsx",
      "!src/index.js",
      "!src/firebase.prod.js",
      "!src/hooks/*.js",
      "!src/context/*.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 90,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "coverageReporters": [
      "html",
      "text"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
